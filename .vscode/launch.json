  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "denon",
      "runtimeExecutable": "denon",
      "runtimeArgs": ["start"],
      "port": 9229,
      "console": "integratedTerminal"
    },
    {
      "name": "Deno (Attach)",
      "type": "node",
      "request": "attach",
      "port": 9229
    },
    {
      "name": "Deno-debugger",
      "type": "pwa-node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--inspect", "-A", "--unstable", "app.ts"],
      "attachSimplePort": 9229,
      "console": "integratedTerminal"
    }
  ]
}
